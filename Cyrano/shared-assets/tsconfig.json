{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "types": ["react", "react-dom"],
    "strict": false,
    "noEmit": true
  },
  "include": ["*.tsx", "*.ts"],
  "exclude": ["node_modules"]
}




