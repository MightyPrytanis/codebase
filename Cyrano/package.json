{
  "name": "cyrano-mcp-server",
  "version": "1.0.0",
  "description": "Cyrano MCP Server - Legal AI and data processing tools with HTTP bridge for integration with LexFiat and other clients",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/http-bridge.js",
    "dev": "tsx src/index.ts",
    "mcp": "tsx src/mcp-server.ts",
    "http": "tsx src/http-bridge.ts",
    "http:simple": "tsx src/simple-http-bridge.ts",
    "http:full": "tsx src/http-bridge.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "./test-integration.sh",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:mcp": "vitest run tests/mcp-compliance",
    "test:watch": "vitest watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@aws-sdk/client-s3": "^3.964.0",
    "@iconify-icons/mingcute": "^1.2.9",
    "@iconify/react": "^6.0.2",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "bcrypt": "^6.0.0",
    "compromise": "^14.14.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "docx": "^9.5.1",
    "dompurify": "^3.3.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.5.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "form-data": "^4.0.1",
    "googleapis": "^169.0.0",
    "helmet": "^8.1.0",
    "jsdom": "^27.4.0",
    "json-rules-engine": "^7.3.1",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.562.0",
    "mammoth": "^1.11.0",
    "marked": "^17.0.1",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "openai": "^6.15.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^2.4.5",
    "pdfjs-dist": "^5.4.530",
    "playwright": "^1.57.0",
    "postgres": "^3.4.8",
    "puppeteer": "^24.35.0",
    "react-icons": "^5.5.0",
    "tesseract.js": "^7.0.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.48.0",
    "@types/cors": "^2.8.19",
    "@types/form-data": "^2.5.2",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^11.0.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.13",
    "csv-parse": "^6.1.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "imapflow": "^1.2.4",
    "patch-package": "^8.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vitest": "^4.0.13"
  }
